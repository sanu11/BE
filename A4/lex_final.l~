%{

#include "y.tab.h"

%}

%%

main	{return MAIN;}
for 	{return FOR;}
if	{return IF;}
else	{return ELSE;}
int|float|char|double	{return DATATYPE;}
printf    {return PRINT;}

[0-9]+ 	{return NUM;}
[a-zA-Z][a-zA-Z0-9]*	{return IDENTIFIER;}
[+-/*]	{return OP;}

\+\+|\-\-	{return INC;}
">"|"<"	{return RELOP;}
\&\&|\|\| {return LOGOP;}
\"[a-zA-Z0-9]*\" {return STCONST;}

[ \n\t]+	;
.	{return yytext[0];}

%%

int yywrap()
{
	return 1;

}
